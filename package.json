{
  "name": "@tech-sumit/mcp-webmcp",
  "version": "0.1.0",
  "description": "MCP server bridging browser WebMCP tools to desktop MCP clients",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "mcp-webmcp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write 'src/**/*.ts'",
    "start": "node dist/cli.js start",
    "start:stdio": "node dist/cli.js",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@tech-sumit/ai-inspector-types": "^0.2.0",
    "@tech-sumit/webmcp-cdp": "github:tech-sumit/webmcp-cdp",
    "commander": "^13.0.0",
    "express": "^5.1.0",
    "playwright": "^1.52.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "mcp",
    "webmcp",
    "mcp-webmcp",
    "chrome-devtools-protocol",
    "tool-bridge"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tech-sumit/webmcp-cdp",
      "esbuild"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tech-sumit/mcp-webmcp"
  }
}
